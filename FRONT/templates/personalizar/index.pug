html(lang="es-CL" dir="ltr" xmlns:og="http://opengraphprotocol.org/schema/" xmlns:fb="http://ogp.me/ns/fb#" heightfull)

  head
    title Personalizar Asistente. MAD Cognitiva

    include ../includes/meta.pug

  body.no-scroll.personalizar(heightfull)
  
    #wrapper(heightfull)
      .f-row(heightfull)
        include ../includes/menu.pug
        .f-col.content(padding heightfull)
          include ../includes/user.pug
          .row
            .col-md-12
              .container-tabs
                .block#custom
                  .custom-col#url
                    .form(no-margin)
                      .row
                        .col-sm-6
                          fieldset
                            label Título
                            input#title-assistant(type="text", value="Asistente Cognitivo", onkeyup="changeText(this)")
                        .col-sm-6
                          fieldset
                            label Sub Título
                            input#subtitle-assistant(type="text", value="Cognitiva", onkeyup="changeText(this)")
                      .row
                        .col-sm-12
                          fieldset
                            label Saludo inicial
                            input#saludo-assistant(type="text", value="Saludo inicial", onkeyup="changeText(this)")
                      .row
                        .col-sm-6
                          fieldset
                            label Logotipo asistente
                            .f-row
                              .f-col
                                label.like-img
                                  | Subir imagen
                                  i.fa.fa-file-image-o(aria-hidden="true")
                                  input#img-logotipo(type="file", accept="image/x-png,image/jpg,image/jpeg", onchange="changeImg(this)")
                              .f-col.f-logo
                                .img-file#logotipo
                                  img(src="data:image/png;base64,", alt="")

                        .col-sm-6
                          fieldset
                            label Avatar empresa
                            .f-row
                              .f-col
                                label.like-img
                                  | Subir imagen
                                  i.fa.fa-file-image-o(aria-hidden="true")
                                  input#img-avatar(type="file", accept="image/x-png,image/jpg,image/jpeg", onchange="changeImg(this)")
                              .f-col.f-logo
                                .img-file#avatar
                                  img(src="data:image/png;base64,", alt="")
                      .row
                        .col-sm-6
                          fieldset
                            label Fondo cabecera asistente
                            input.colors#color-header(type="text", value="#1c75bc", onchange="getChangeColor(this)", style="background-color: #1c75bc;")
                        .col-sm-6
                          fieldset
                            label Fondo burbuja asistente
                            input.colors#color-bubble(type="text", value="#e7e7e7", onchange="getChangeColor(this)", style="background-color: #e7e7e7;")
                      .row
                        .col-sm-6
                          fieldset
                            label Fondo botones asistente
                            input.colors#color-buttons(type="text", value="#009dc3", onchange="getChangeColor(this)", style="background-color: #009dc3;")
                    button#save-custom(button onclick="saveCustomParams()") Guardar
                  .custom-assistant
                    .assistant
                      .header
                        .img
                          img#header-logotipo(src="../assets/img/brain_cognitiva.png", alt="")
                        .titles
                          div Asistente Cognitivo
                          span Cognitiva
                        .close
                          i.fa.fa-times
                      iframe#demo-custom(src="", frameborder="0")
                  include ../includes/spinner.pug
    
    include ../includes/modal.pug
    include ../includes/modal-script.pug

    script(type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js")
    script(type="text/javascript" src= myPath +'/assets/js/bootstrap.min.js')
    script(type="text/javascript" src= myPath +'/assets/js/bootstrap-colorpicker.min.js')
    script(type="text/javascript" src= myPath +'/assets/js/main.js')
    script.
      $(document).ready(function(){
        $('#tabs a').click(function (e) {
          e.preventDefault()
          $(this).tab('show')
        });
        $('ul.list-intents li div a:first-child').click(function(){
          $(this).parents('li').find('ul').slideToggle(300);
          $(this).find('.fa').toggleClass('fa-rotate-90');
          return false;
        });
        $('#color-buttons, #color-bubble, #color-header').colorpicker({
          useAlpha: false,
          enableHex8: false
        });
      });
      
      //- var getIdUser = sessionStorage.getItem('user_id');
      var getUrlDemo = getPath+'asistente/';
      var getDemo = document.getElementById('demo-custom'),
          getTkn = sessionStorage.getItem('tkn');
          loadAll = 0;
          //- getTheToken = '';

      //- if (sessionStorage.getItem('tkn')) {
      //-   getTheToken = '?token='+sessionStorage.getItem('tkn');
      //- } else {
      //-   getTheToken = '';
      //- }

      getDemo.attributes[1].value = getUrlDemo;

      function spinnerOut(parent){
        $(parent+' .spinner').fadeOut(100);
      }

      function getChangeColor(el) {
        var getColor = $(el).val();
        $(el).css({backgroundColor: getColor});
        if(el.id == 'color-header') {
          $('.assistant .header').css({backgroundColor: getColor});
        } else if(el.id == 'color-bubble'){
          $('.assistant .bubble').css({backgroundColor: getColor});
        } else {
          $('.assistant .send').css({backgroundColor: getColor});
        }
      }

      var getId = "1";

      function changeText(el){
        var getText = $(el).val();
        if(el.id == 'title-assistant'){
          $('.assistant .titles div').text(getText);
        } else {
          $('.assistant .titles span').text(getText);
        }        
      }

      var uploadImgLogo   = '';
      var uploadImgAvatar = '';

      function changeImg(el) {
        var getImg = $(el).val();
        var _URL = window.URL || window.webkitURL;

        if(el.id == 'img-logotipo'){
          var reader = new FileReader();
          var img = new Image();
          img.onload = function(){
            if(img.naturalHeight > 200 || img.naturalWidth > 200) {
              openModal('#myModal', 'Lo sentimos', 'La imagen debe medir 200 x 200 píxeles como máximo.', 'error');
              el.value = '';
            } else {
              reader.onload = function(e) {
                uploadImgLogo = reader.result;
                $('#logotipo img').attr('src', e.target.result);
                //- $('#header-logotipo').attr('src', e.target.result);
              }          
              reader.readAsDataURL(el.files[0]);
            }
          }
          img.src = _URL.createObjectURL(el.files[0]);
        } else { 
          var reader = new FileReader();
          var img = new Image();
          img.onload = function(){
            if(img.naturalHeight > 200 || img.naturalWidth > 200) {
              openModal('#myModal', 'Lo sentimos', 'La imagen debe medir 200 x 200 píxeles como máximo.', 'error');
              el.value = '';
            } else {
              reader.onload = function(e) {
                uploadImgAvatar = reader.result;
                $('#avatar img').attr('src', e.target.result);
              }          
              reader.readAsDataURL(el.files[0]);
            }
          }
          img.src = _URL.createObjectURL(el.files[0]);
        }
      }

      function openModal(modal, title, body, theclass) {
        $(modal).find('h6').text(title);
        $(modal).find('.modal-body').empty().append(body);
        $(modal).addClass(theclass);
        $(modal).modal({
          backdrop: 'static',
          keyboard: false,
          show: true
        });
      }

      function closeCustomModal(modal){
        $(modal).find('.container-form').removeClass('active');
        $(modal).delay(200).fadeOut(200);
        $(modal).find('.buttons, form').fadeOut(50);
      }

      //- function postMessage(msg){
      //-   getDemo.contentWindow.postMessage(msg, "*");
      //- }

      function getMessageResponse() {
        const promise = $.ajax({
          async: true,
          type: 'POST',
          url: getPath+"mad/message",
          crossDomain: true,
          data: JSON.stringify({ general: { cid: null, id_cliente: "1" }, msg: null }),
          contentType: 'application/json',
          dataType: 'json'
        });
        promise.done((response) => {
          $('#saludo-assistant').val(response.msg);
          loadAll = loadAll + 1;
          if(loadAll===2){
            spinnerOut('#custom');
          }      
        }).fail(() => {

        });
        return promise;
      }

      function getCustomParams() {

        var getSession           = "1";
        var getLogoAssistant     = '';
        var getAvatarAssistant   = '';
        var getButtonColor       = '';
        var getHeaderColor       = '';
        var imgLogo              = document.getElementById('img-logotipo');
        var imgAvatar            = document.getElementById('img-avatar');
        var reader               = new FileReader();
        var img                  = new Image();
        var _URL                 = window.URL || window.webkitURL;

        var args = {
          id_cliente: "1"
        }
        var promise = $.ajax({
          type: 'POST',
          url: getPath+"mad/customize_param",
          data: JSON.stringify(args),
          "headers": {
            "Content-Type": "application/json",
            "Authorization": "Bearer "+getTkn
          },
          dataType: 'json',
          contentType: 'application/json; charset=utf-8'
        }).done(function(response){
          if(response.status == '200'){
            
            getMessageResponse();

            $('#color-bubble').val(response.colorBubble);
            $('#color-bubble, .assistant .bubble').css("background-color", response.colorBubble);
            $('#color-header').val(response.colorHeader);
            $('#color-header, .assistant .header').css("background-color", response.colorHeader)
            $('#color-buttons').val(response.colorBtn);
            $('#color-buttons, .assistant .send').css("background-color", response.colorBtn)
            $('#title-assistant').val(response.titulo);
            $('.assistant .titles div').text(response.titulo);
            $('#subtitle-assistant').val(response.subtitulo);
            $('.assistant .titles span').text(response.subtitulo);
            document.getElementById('header-logotipo').setAttribute('src', response.logo);
            $('#logotipo img').attr('src', response.logo);
            $('#avatar img').attr('src', response.avatar);

            spinnerOut('#custom');
          }
        })
      }

      function loadCustom(customParams){
        $('#color-bubble').val(customParams.colorBubble);
        $('#color-bubble, .assistant .bubble').css("background-color", customParams.colorBubble);
        $('#color-header').val(customParams.colorHeader);
        $('#color-header, .assistant .header').css("background-color", customParams.colorHeader)
        $('#color-buttons').val(customParams.colorBtn);
        $('#color-buttons, .assistant .send').css("background-color", customParams.colorBtn);
        loadAll = loadAll + 1;
        if(loadAll===2){
          spinnerOut('#custom');
        }
      }

      function init(){
        window.onmessage = e => {
          console.log("onmessage Mant",e.data)
          if (e.data.customParams !== undefined) {
            loadCustom(e.data.customParams);
          }
        };
        getMessageResponse();
      }

      window.onload = function() {
        init();
        //getCustomParams();
        var getSession = "1";
        $('#assistant-iframe').attr('src', getPath+'asistente/');
      }

      function saveCustomParams() {

        var form           = new FormData();
        var getTitle       = document.getElementById('title-assistant').value;
        var getSubTitle    = document.getElementById('subtitle-assistant').value;
        var colorHeader    = document.getElementById('color-header').value;
        var colorBuble     = document.getElementById('color-bubble').value;
        var colorButtons   = document.getElementById('color-buttons').value;
        var imgLogo        = $('#logotipo img').attr('src');
        var imgAvatar      = $('#avatar img').attr('src');
        if(getTitle == '' || getSubTitle == '' || colorHeader == '' || colorBuble == '' || colorButtons == '') {
          openModal('#myModal', 'Lo sentimos', 'Debes completar todos los campos', 'error');
        } else {
          var args = {
            titulo: getTitle,
            subtitulo: getSubTitle,
            color_fondo: colorHeader,
            color_fondo_chat: colorBuble,
            color_btn: colorButtons,
            id_empresa: getId,
            logo: imgLogo,
            avatar: imgAvatar
          }
          var promise = $.ajax({
            type: 'POST',
            url: getPath+"mant/customize",
            data: JSON.stringify(args),
            "headers": {
              "Content-Type": "application/json",
              "Authorization": "Bearer "+getTkn
            },
            dataType: 'json',
            contentType: 'application/json; charset=utf-8'
          }).done(function(response){
            if(response.status == 200){
              openModal('#myModal', 'Genial!', 'Tu asistente se ha personalizado con éxito', 'success');
              //- localStorage.setItem('avatarAssistant', uploadImgLogo);
              //- localStorage.setItem('logoAssistant', uploadImgAvatar);
              //- localStorage.setItem('colorHeader', colorHeader);
              //- localStorage.setItem('colorBubble', colorBuble);
              //- localStorage.setItem('colorButtons', colorButtons);
              //- localStorage.setItem('subTitleAssistant', getSubTitle);
              //- localStorage.setItem('titleAssistant', getTitle);
              getCustomParams();
              getDemo.src = getDemo.src;
            } else {
              openModal('#myModal', 'Auch!', 'Tenemos un problema, favor intenta nuevamente', 'error');
            }
          })
        }
      }


